{
    "name": "apxcode/wink",
    "description": "Wink Publishing Framework.",
    "keywords": ["framework", "laravel", "blog"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mwamodo Erick",
            "email": "erick@apexcode.dev"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0||^11.0"
    },
    "autoload": {
        "psr-4": {
            "Wink\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Wink\\WinkServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
